<!-- launch file for part of the challenge: apriltag, planners -->
<launch>
    <arg name="sim" default="true" doc="Define if it is simulation environment or not"/>

    <include if="$(arg sim)" file="$(find challenge_arena)/launch/apriltag.launch"/>

    <include if="$(arg sim)" file="$(find ur10_platform_challenge_moveit_config)/launch/ur10_platform_challenge_moveit_planning_execution.launch">
        <arg name="sim" value="$(arg sim)"/>
    </include>

    <include file="$(find g01_move)/launch/robot_navigation.launch">
	<arg name="sim" value="$(arg sim)"/>
    </include>
</launch>
